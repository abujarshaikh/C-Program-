//slip no.5,7 and 15
//que.1
//swaping using pointer
#include<stdio.h>
#include<conio.h>
void main()
{
int a,b,*x,*y, temp;
 
printf("Enter a and b:");
scanf("%d%d",&a,&b);
printf("Before te swapnig\nThe value of a=%d\nThe value of b=%d\n",a,b);
x=&a;
y=&b;
temp=*y;
*y=*x;
*x=temp;
printf("\n\nAfter the swapnig\nThe value of a=%d\nThe value of b=%d",*x,*y);
/*sum=*ptr_a+*ptr_b; 
printf("\nSum = %d",sum);*/
getch();
}
